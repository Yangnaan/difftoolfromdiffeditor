{
  "name": "difftoolfromdiffeditor",
  "displayName": "DifftoolFromDiffEditor",
  "description": "launch difftool form diff editor",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-difftool-button.openInGitDifftool",
        "title": "Open with Git Difftool",
        "enablement": "isInDiffEditor",
        "category": "Git",
        "icon": "$(diff)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "isInDiffEditor || resourceScheme == git",
          "command": "git-difftool-button.openInGitDifftool",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  }
}
